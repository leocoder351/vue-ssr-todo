    "babel-plugin-transform-vue-jsx": "3.7.0",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "5.6.1",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-html": "4.0.6",
